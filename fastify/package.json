    "docker:run": "docker run --rm -p 3000:3000 fastify-api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^4.0.0",
    "fastify-plugin": "^4.0.0",
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "fastify-cli": "^5.4.1"
  },
  "devDependencies": {
    "tap": "^16.1.0"
  }
}